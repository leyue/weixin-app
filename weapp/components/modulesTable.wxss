  background: white;
  font-size: 0.8rem;
}
.tr {
  display: flex;
  width: 100%;
  height: 1.2rem;
  align-items: center;
  justify-content: center;
}

.th {
  background: #4770f7;
  color: #fff;
}

.td {
  width: 30%;
  padding: 5px;
  overflow: scroll;
}

.line {
  display: flex;
  height: 1.2rem;
  align-items: flex-start;
  justify-content: flex-start;
}
.label {
  margin: 0 10px;
  font-weight: bold;
  width: 30%;
}

